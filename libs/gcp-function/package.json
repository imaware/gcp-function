{
  "name": "@imaware/gcp-function",
  "version": "0.0.4",
  "author": "Christopher Manzi <chris@imaware.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imaware/gcp-function"
  },
  "homepage": "https://github.com/imaware/gcp-function",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "schematics": "./collection.json",
  "builders": "./builders.json",
  "peerDependencies": {
    "@nrwl/workspace": "*"
  },
  "scripts": {
    "release:clean": "rimraf dist",
    "release:copy": "run-s release:copy-misc release:copy-http-files release:copy-http-schema release:copy-http-src release:copy-pubsub-files release:copy-pubsub-schema release:copy-pubsub-src release:copy-builders",
    "release:copy-misc": "copyfiles builders.json collection.json package.json README.md dist",
    "release:copy-http-schema": "copyfiles -f src/schematics/gcp-function-http/schema.json dist/src/schematics/gcp-function-http",
    "release:copy-http-files": "copyfiles -f src/schematics/gcp-function-http/files/** dist/src/schematics/gcp-function-http/files",
    "release:copy-http-src": "copyfiles -f src/schematics/gcp-function-http/files/src/** dist/src/schematics/gcp-function-http/files/src",
    "release:copy-pubsub-schema": "copyfiles -f src/schematics/gcp-function-pubsub/schema.json dist/src/schematics/gcp-function-pubsub",
    "release:copy-pubsub-files": "copyfiles -f src/schematics/gcp-function-pubsub/files/** dist/src/schematics/gcp-function-pubsub/files",
    "release:copy-pubsub-src": "copyfiles -f src/schematics/gcp-function-pubsub/files/src/** dist/src/schematics/gcp-function-pubsub/files/src",
    "release:copy-builders": "copyfiles -f src/builders/gcp-function/schema.json src/builders/gcp-function/files dist/src/builders/gcp-function",
    "release:build": "tsc --project ./tsconfig.lib.json",
    "release:prepare": "run-s release:clean release:build release:copy",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@angular-devkit/architect": "~0.901.0",
    "@angular-devkit/build-webpack": "~0.901.0",
    "@angular-devkit/core": "~9.1.0",
    "@angular-devkit/schematics": "~9.1.0",
    "@google-cloud/functions-framework": "^1.5.1",
    "@google-cloud/pubsub": "^2.10.0",
    "@nrwl/jest": "*",
    "@nrwl/linter": "*",
    "@nrwl/node": "*",
    "@types/express": "*",
    "circular-dependency-plugin": "5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "decimal.js": "^10.2.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "generate-package-json-webpack-plugin": "^1.0.1",
    "googleapis": "^67.1.1",
    "license-webpack-plugin": "2.1.2", 
    "moment": "^2.24.0",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "tree-kill": "1.2.2",
    "ts-loader": "5.4.5",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslib": "^1.11.1",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.0",
    "shx": "^0.3.3",
    "tsc": "^1.20150623.0",
    "typescript": "^4.2.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@imaware"
  }
}
